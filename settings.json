{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://raw.githubusercontent.com/microsoft/terminal/main/doc/cascadia/profiles.schema.json",
    "actions": 
    [
        {
            "command": 
            {
                "action": "adjustOpacity",
                "opacity": 95,
                "relative": false
            },
            "keys": "ctrl+shift+0"
        },
        {
            "command": 
            {
                "action": "commandPalette"
            },
            "keys": "f1"
        },
        {
            "command": 
            {
                "action": "swapPane",
                "direction": "left"
            },
            "keys": "alt+shift+left"
        },
        {
            "command": 
            {
                "action": "swapPane",
                "direction": "down"
            },
            "keys": "alt+shift+down"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "ctrl+alt+left"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "ctrl+alt+down"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "ctrl+alt+right"
        },
        {
            "command": 
            {
                "action": "swapPane",
                "direction": "up"
            },
            "keys": "alt+shift+up"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+v"
        },
        {
            "command": 
            {
                "action": "swapPane",
                "direction": "right"
            },
            "keys": "alt+shift+right"
        },
        {
            "command": 
            {
                "action": "adjustOpacity",
                "opacity": 5,
                "relative": true
            },
            "keys": "ctrl+shift+plus"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "ctrl+alt+up"
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+shift+z"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+h"
        },
        {
            "command": "markMode",
            "keys": "ctrl+shift+["
        },
        {
            "command": "toggleBlockSelection",
            "keys": "ctrl+q"
        },
        {
            "command": 
            {
                "action": "adjustOpacity",
                "opacity": -5,
                "relative": true
            },
            "keys": "ctrl+shift+minus"
        }
    ],
    "centerOnLaunch": true,
    "confirmCloseAllTabs": false,
    "copyFormatting": "none",
